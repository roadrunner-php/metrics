    "autoload-dev": {
        "psr-4": {
            "Spiral\\RoadRunner\\Metrics\\Tests\\": "tests"
        }
    },
    "scripts": {
        "tests": "phpunit",
        "psalm": "psalm --no-cache"
    },
    "suggest": {
        "spiral/roadrunner-cli": "Provides RoadRunner installation and management CLI tools"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
